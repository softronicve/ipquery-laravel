{
    "name": "softronicve/ipquery-laravel",
    "description": "Laravel package for ipquery.io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Alvarado",
            "email": "leonardoalvarado2013@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Softronic\\Ipquery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softronic\\Ipquery\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Softronic\\Ipquery\\IpqueryServiceProvider"
            ],
            "aliases": {
                "Ipquery": "Softronic\\Ipquery\\Facades\\Ipquery"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}