{
    "name": "softronicve/ipquery-laravel",
    "description": "Laravel package for ipquery.io",
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Alvarado",
            "email": "leonardoalvarado2013@gmail.com"
        },
        {
            "name": "Softronic",
            "email": "desarrollosoftronic@gmail.com",
            "homepage": "https://www.softronic.dev/"
        }
    ],
    "homepage": "https://github.com/softronicve/ipquery-laravel",
    "keywords": ["Laravel", "ipquery","ipquerylaravel"],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Softronic\\Ipquery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softronic\\Ipquery\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Softronic\\Ipquery\\IpqueryServiceProvider"
            ],
            "aliases": {
                "Ipquery": "Softronic\\Ipquery\\Facades\\Ipquery"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}